{
  "name": "@advanced/mosaic-components",
  "version": "18.0.0",
  "description": "Mosaic web components",
  "scripts": {
    "build": "run-p build:*",
    "build:components": "cross-env NODE_ENV=production CDN_SERVER=https://cdn.svc.oneadvanced.com rollup -c rollup.config.components.js",
    "build:deps": "cross-env NODE_ENV=production rollup -c rollup.config.deps.js",
    "build:polyfills": "cross-env NODE_ENV=production rollup -c rollup.config.polyfills.js",
    "build:helpers": "cross-env NODE_ENV=production rollup -c rollup.config.helpers.js",
    "build:loader": "cross-env NODE_ENV=production rollup -c rollup.config.loader.js",
    "build:sass": "node-sass --importer node_modules/node-sass-tilde-importer --output ./tmp/css ./src/sass",
    "build:snippets": "node scripts/gen-snippets.js",
    "gen:all-tags": "node scripts/gen-all-tags.js",
    "storybook:build": "build-storybook -c .storybook-cdn -o ../../apps/storybook/dist",
    "prestorybook:build": "npm run clean:storybook && npm run predev",
    "poststorybook:build": "npm run copy:storybook:public && npm run copy:storybook:components",
    "prebuild": "run-p gen:all-tags copy:*",
    "prebuild:sass": "rimraf tmp/css",
    "prebuild:snippets": "mkdirp tmp/snippets && mkdirp dist/snippets/visual-studio-code && mkdirp dist/snippets/visual-studio && mkdirp dist/snippets/intellij && mkdirp dist/snippets/webstorm && mkdirp dist/snippets/eclipse && mkdirp dist/snippets/codepen",
    "postbuild:sass": "cross-env NODE_ENV=production postcss ./tmp/css --dir ./dist/css",
    "lint:sass": "stylelint \"src/sass/**/*.scss\"",
    "lint:js": "eslint \"src/**/*.js\"",
    "ci": "start-server-and-test storybook:prod http://localhost:9001 cy:run",
    "ci:local": "start-server-and-test storybook:prod http://localhost:9001 cy:local",
    "clean": "run-p clean:*",
    "clean:dist": "rimraf dist",
    "clean:tmp": "rimraf tmp",
    "clean:tmpcss": "rimraf tmp-css",
    "clean:cypress": "rimraf cypress/screenshots cypress/videos cypress/reports",
    "clean:storybook": "rimraf ../../apps/storybook/dist",
    "clean:storybook:snippets": "rimraf ../../apps/storybook/dist/mosaic-components/snippets && rimraf ../../apps/storybook/dist/mosaic-components/snippets.json && rimraf ../../apps/storybook/dist/mosaic-components/all-snippets.json",
    "clean:storybook:gzipped": "rimraf ../../apps/storybook/dist/mosaic-components/**/*.gz",
    "copy:storybook:public": "ncp public/ ../../apps/storybook/dist/",
    "copy:storybook:components": "ncp dist ../../apps/storybook/dist/mosaic-components",
    "postcopy:storybook:components": "run-p clean:storybook:*",
    "cy:open": "cypress open",
    "cy:run": "cypress run",
    "cy:run:no-video": "cypress run --config video=false",
    "cy:run:chrome": "cypress run --browser chrome",
    "cy:local": "run-s cy:local:*",
    "cy:local:controls": "node scripts/cypress-tests-by-bundle.js controls",
    "cy:local:core": "node scripts/cypress-tests-by-bundle.js core",
    "cy:local:data": "node scripts/cypress-tests-by-bundle.js data",
    "cy:local:forms": "node scripts/cypress-tests-by-bundle.js forms",
    "cy:notlocal:layout": "node scripts/cypress-tests-by-bundle.js layout",
    "cy:local:messaging": "node scripts/cypress-tests-by-bundle.js messaging",
    "cy:notlocal:pages": "node scripts/cypress-tests-by-bundle.js pages",
    "cy:local:scaffold": "node scripts/cypress-tests-by-bundle.js scaffold",
    "cy:notlocal:tools": "node scripts/cypress-tests-by-bundle.js tools",
    "precy:run": "npm run clean:cypress",
    "precy:run:local": "npm run clean:cypress",
    "copy:vendor": "node scripts/copy-vendor.js",
    "precopy:vendor": "mkdirp dist/vendor",
    "docs": "jsdoc -r src/util -d docs -R src/util/README.md",
    "predev": "ncp dist/snippets.json public/snippets.json",
    "dev": "run-p gen:all-tags dev:watch storybook serve dev:open",
    "dev:watch": "run-p watch:*",
    "dev:open": "open-cli http://localhost:8080/examples/html/_storybook-build.html",
    "dev:open:comp": "open-cli http://localhost:8080/examples/html/_components-build.html",
    "dev:comp": "run-p dev:watch serve dev:open:comp",
    "serve": "node scripts/server.js",
    "storybook": "start-storybook -p 9001 -s ./public --ci",
    "storybook:prod": "start-storybook -p 9001 -c ./.storybook-prod -s ./dist,./public,./node_modules/@advanced --ci",
    "test": "run-p storybook:prod test:wait",
    "test:wait": "wait-on http://localhost:9001 && npm run cy:open",
    "dev:scss:compile": "node-sass --include-path ./node_modules ./src/sass --recursive --source-map true --importer node_modules/node-sass-tilde-importer --output ./tmp-css/css",
    "postdev:scss:compile": "postcss ./tmp-css/css/*.css --dir ./tmp/css",
    "prewatch:sass": "npm run dev:scss:compile",
    "watch:sass": "chokidar \"src/sass/**/*.scss\" -c \"node scripts/sass-compile.js \"\"{path}\"\"\"",
    "watch:deps": "cross-env NODE_ENV=development rollup -c rollup.config.deps.js -w",
    "watch:polyfills": "cross-env NODE_ENV=development rollup -c rollup.config.polyfills.js -w",
    "watch:helpers": "cross-env NODE_ENV=development rollup -c rollup.config.helpers.js -w",
    "watch:loader": "cross-env NODE_ENV=development rollup -c rollup.config.loader.js -w",
    "watch:components": "cross-env NODE_ENV=development rollup -c rollup.config.components.js -w"
  },
  "author": "Advanced",
  "license": "ISC",
